{
  "name": "dev-a-day",
  "version": "0.0.0",
  "scripts": {
    "start": "ng serve",
    "open": "ng serve --open",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "build:deploy": "ng build --prod --base-href \"https://blog.aaronross.tech/\"",
    "deploy": "yarn build:deploy && angular-cli-ghpages --dir=dist/dev-a-day",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "core-js": "^2.5.4",
    "date-fns": "^2.0.0-alpha.27",
    "feather-icons": "^4.16.0",
    "jquery": "^3.3.1",
    "ngx-markdown": "^7.1.3",
    "normalize.css": "^8.0.1",
    "prismjs": "^1.15.0",
    "rxjs": "~6.3.3",
    "slugify": "^1.3.4",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^7.2.0",
    "@angular-builders/dev-server": "^7.2.1",
    "@angular-devkit/build-angular": "^0.13.0",
    "@angular/cli": "~7.3.0",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@types/jest": "^23.3.13",
    "@types/node": "~8.9.4",
    "angular-cli-ghpages": "^0.5.3",
    "jest": "^24.0.0",
    "jest-preset-angular": "^6.0.2",
    "prettier": "^1.16.3",
    "raw-loader": "^1.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupJest.ts"
    ]
  }
}
